		{
			"path": "pages/entrance/entrance"
		},
		{
			"path": "pages/guard/guard"
		},
		{
			"path": "pages/profile/profile"
		},
		{
			"path": "pages/agreement/agreement"
		}, {
			"path": "pages/search/search"
		}, {
			"path": "pages/assistant/assistant"
		}, {
			"path": "pages/checkcode-result/checkcode-result"
		},
		{
			"path": "pages/detail/detail",
			 "style": {
			        "navigationBarTitleText": "信息详情"//设置页面标题文字
